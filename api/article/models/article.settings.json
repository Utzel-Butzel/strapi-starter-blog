{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "articles"
    },
    "subtitle": {
      "type": "string"
    },
    "detailimage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "meta": {
      "type": "component",
      "repeatable": true,
      "component": "meta.meta"
    },
    "partner": {
      "type": "component",
      "repeatable": true,
      "component": "partner.partner"
    },
    "slug": {
      "type": "string"
    }
  }
}
